	android:layout_height="wrap_content">
	
	<ImageView android:id="@+id/title_pic" android:src="@drawable/tabcontact"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_gravity="center">
	</ImageView>
	<TextView android:id="@+id/space0" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textSize="20sp" 
		android:text=" "/>
	<TextView android:id="@+id/row_entry" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textSize="20sp" />
	<TextView android:id="@+id/space" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textSize="20sp" 
		android:text=" ("/>
	<TextView android:id="@+id/row_entry2" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textSize="20sp" />
	<TextView android:id="@+id/space2" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textSize="20sp" 
		android:text=")"/>
	
</LinearLayout>
